function setup() { 
  createCanvas(410, 410);
} 

function draw() { 
  background(255);
	strokeWeight(1);
	stroke(0);


	line(0, 400, 0, 0);
	line(0, 0, 400, 0);
	line(0, 400, 400, 400);
	line(400, 0, 400, 400);
	
	line(100, 100, 300, 300);
	line(300, 100, 100, 300);


